{
    "name": "incident-io-alert",
    "version": "0.0.0",
    "private": false,
    "type": "module",
    "description": "A community maintained GitHub Action for triggering Incident.io alerts from GitHub workflows.",
    "main": "lib/runtime.js",
    "exports": {
        ".": "./dist/index.js"
    },
    "engines": {
        "node": ">=24.0.0"
    },
    "scripts": {
        "build:compile": "tsc",
        "build:package": "ncc build --license licenses.txt",
        "lint:style": "prettier --check **/*.ts",
        "lint:static-analysis": "eslint src/**/*.ts",
        "test:unit": "NODE_OPTIONS=--experimental-vm-modules NODE_NO_WARNINGS=1 jest"
    },
    "keywords": [
        "incident.io",
        "alert",
        "paging"
    ],
    "author": "Ryan Maber",
    "license": "MIT",
    "dependencies": {
        "@actions/core": "^3.0.0",
        "@actions/github": "^9.0.0",
        "@actions/http-client": "^3.0.1",
        "@types/node": "^25.0.3",
        "crypto": "^1.0.1"
    },
    "devDependencies": {
        "@jest/globals": "^30.2.0",
        "@types/jest": "^30.0.0",
        "@typescript-eslint/eslint-plugin": "^8.53.1",
        "@vercel/ncc": "^0.38.4",
        "eslint": "^10.0.0",
        "eslint-plugin-github": "^6.0.0",
        "eslint-plugin-jest": "^29.12.1",
        "jest": "^30.2.0",
        "js-yaml": "^4.1.1",
        "prettier": "^3.8.1",
        "ts-jest": "^29.4.6",
        "ts-jest-resolver": "^2.0.1",
        "typescript": "^5.9.3"
    }
}